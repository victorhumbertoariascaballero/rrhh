@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>SISPAD </title>

    <!-- Bootstrap -->
    <link href="~/Content/vendor/css/bootstrap.min.css" rel="stylesheet">
    <!-- Bootstrap -->
    <!-- Bootstrap -->
    <link href="~/Content/vendor/css/dataTables.bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="~/Content/vendor/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="../vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- Animate.css -->
    <link href="../vendors/animate.css/animate.min.css" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="~/Content/css/custom.min.css" rel="stylesheet">
    <!-- jQuery -->
    <script src="~/Content/vendor/js/jquery.min.js"></script>
</head>
<body class="login">

    @Html.Hidden("UrlPaginaPorDefecto", Url.Action("Index", "Home"))
    @Html.Hidden("UrlChangePasswordConfirm", Url.Action("ChangePasswordConfirm", "Account"))
    <div>
        <a class="hiddenanchor" id="signup"></a>
        <a class="hiddenanchor" id="signin"></a>
        <div class="login_wrapper">
            <div class="animate form login_form">
                <section class="login_content">
                    <img src="~/Content/Images/logo-midis.jpg" class="m-b-sm">
                    <h2>SISTEMA DE PROCEDIMIENTO ADMINISTRATIVO DISCIPLINARIO</h2>

                    <div class="hpanel">
                        <div class="panel-body">
                            <input type="hidden" value="@ViewBag.vToken" id="vToken" />
                            <div>
                                <span> Estimado usuario, registre una nueva contraseña</span>
                            </div>
                            <div>
                                <input type="password" id="vPassword" class="form-control" placeholder="Ingresa tu nueva clave" required="" />
                            </div>
                            <div>
                                <input type="password" id="vRepetirPassword" class="form-control" placeholder="Repite tu nueva clave" required="" />
                            </div>
                            <div>
                                <label id="lblMensaje"></label>
                            </div>
                            <div>
                                <button id="btnActualizarContrasena" class="btn btn-sucess submit" href="javascript:void(0)">Actualizar Contraseña</button>
                                <button id="btnCancelar" class="btn btn-warning submit" href="javascript:void(0)">Página Princial</button>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="separator">
                        <div class="clearfix"></div>
                        <br />
                        <div>
                            <h2> BANDEJA BUZÓN DE SUGERENCIA</h2>
                            <p>© MIDIS.</p>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</body>
</html>
<script src="@Url.Content("~/Scripts/Account/ChangePasswordConfirm.js")" type="text/javascript"></script>