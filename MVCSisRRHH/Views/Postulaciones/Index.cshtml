        border-radius: 3px;
    }

    .form-group {
        margin-bottom: 3px;
    }

    .console {
        background-color: transparent;
        color: #333;
        font: 11px Consolas, Monaco, "Bitstream Vera Sans Mono", "Courier New", Courier, monospace;
        margin: 0;
        overflow-x: hidden;
        text-align: left;
        height: 200px;
        border: 1px solid rgba(20,53,80,0.1);
        background-color: #ffffff;
        text-indent: 0;
    }

    .console div {
        background-position: 6px -95px;
        border-bottom: 1px solid #DDD;
        padding: 5px 10px;
        height: 2em;
        line-height: 2em;
        vertical-align: middle;
    }

    input, label, .k-input {
        font-size: 11px;
    }
    .panel-title {
        margin-top: 0;
        margin-bottom: 0;
        font-size: 16px;
        color: inherit;
    }
    .panel-heading {
        color: #fff !important;
        background-color: #F05454;
        border-color: #FA76B1;
    }
    .k-radio:checked + .k-radio-label:after {
        background-color: #f05454;
        border-radius: 50%;
